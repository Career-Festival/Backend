{
  "containerDefinitions": [
    {
      "name": "careerfestival-backend",
      "image": "euegenechoi/careerfestival-backend:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 9000,
          "containerPort": 9000
        }
      ]
    },
    {
      "name": "careerfestival-frontend",
      "image": "euegenechoi/careerfestival-frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    }
  ],
  "compose": {
    "dockerComposeFile": "docker-compose.yml"
  }
}